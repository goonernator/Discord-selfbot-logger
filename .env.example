# Discord Selfbot Logger Configuration
# Copy this file to .env and fill in your actual values

# Required: Your Discord user token
# WARNING: This is sensitive information. Never commit this file to version control!
DISCORD_TOKEN=YOUR_DISCORD_TOKEN_HERE

# Required: Discord webhook URLs for different event types
WEBHOOK_URL_FRIEND=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN
WEBHOOK_URL_MESSAGE=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN
WEBHOOK_URL_COMMAND=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Optional: Logging configuration
# LOG_LEVEL=INFO

# Optional: Web dashboard configuration
# WEB_HOST=127.0.0.1
# WEB_PORT=5002

# Optional: Attachment download settings
# ATTACHMENT_SIZE_LIMIT=104857600
# MAX_CONCURRENT_DOWNLOADS=5
# ENABLE_ATTACHMENT_DOWNLOAD=true
